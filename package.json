{
    "name": "auto-type",
    "displayName": "auto-type",
    "description": "Auto-type code from a script",
    "version": "0.0.1",
    "publisher": "none",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.playCodeScript",
        "onCommand:extension.resetCodeScript"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.playCodeScript",
                "title": "Play Code Script"
            },
            {
                "command": "extension.resetCodeScript",
                "title": "Restart Code Script"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Auto Type configuration",
            "properties": {
                "auto-type.changes": {
                    "type": "array",
                    "default": [],
                    "description": "List of changes."
                }
            }
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
